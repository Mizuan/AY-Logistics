<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title> 
    <link rel="shortcut icon" href="../../Content/images/aikoclock256x256.ico" />   
    @*CSS*@
      <link href="@Url.Content("~/Content/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/MySite_Cyborg.css")" rel="stylesheet" type="text/css" />@*Custom CSS file*@
    <link href="@Url.Content("~/Content/font-awesome.css")" rel="stylesheet" type="text/css" />@*Menu bar icon*@
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/jquery.pnotify.default.css")" rel="stylesheet" type="text/css" />@*Notification*@
    <link href="@Url.Content("~/Content/MyCustom.css")" rel="stylesheet" type="text/css" />

    @*Jquery*@
    <script src="@Url.Content("~/Scripts/jquery-1.8.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.pnotify.js")" type="text/javascript"></script>@*Notification*@
    <script src="@Url.Content("~/Scripts/bootstrap.js")" type="text/javascript"></script>
    @* Knockout *@
    <script src="@Url.Content("~/Scripts/knockout-2.2.0.debug.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/knockout.mapping-latest.debug.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/mycustom.js")" type="text/javascript"></script>


    
</head>
<body>
    <div class="container-fluid">
        <div class="row-fluid">
            <div>

            </div>
            <div class="span9" style="border-left-width: 0px; padding-left: 100px;">
               @RenderBody()
            </div>
        </div>
    </div> 

        <script type="text/javascript">
            /****Auto redirect to loging Page after session expired*******/
            var timeout = @FormsAuthentication.Timeout.TotalMilliseconds; // time taken from 'Web.config'
            var wintimeout;

            function SetWinTimeout() {
                wintimeout =  window.setTimeout( "window.location.href='@FormsAuthentication.LoginUrl';",timeout); // In millisecods
        }
 
        $('body').click(function() {  
            //when user clicks remove timeout and reset it
            window.clearTimeout(wintimeout);  
            SetWinTimeout();

        });
        SetWinTimeout();
     </script>
</body>
</html>
