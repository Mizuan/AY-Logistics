         self.UpdateDemurrageDate  = function(data){
             var date2 = $('.d_disDate').datetimepicker('getDate', '+2d'); 
             var weekDay = date2.getDay();
             if(self.ModeofShipment()==1)
             {
                 $.each(self.HouseBLItems(), function() {
