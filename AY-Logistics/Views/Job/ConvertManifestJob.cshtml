                                 case 6: //Saturday
                                     //no day added
                                     break;
                                 default:
                             }
                         }else{
                             date2.setDate(date2.getDate()+10);
                             switch(weekDay) { // if 10 Days
                                 case 0: // Sunday
                                 case 5: //Friday
                                 case 6: //Saturday
                                     date2.setDate(date2.getDate()+1);
                                     break;
                                 case 1: //Monday
                                 case 2: //Tuesday
                                 case 3: //Wednesday
                                 case 4: //Thursday
                                     date2.setDate(date2.getDate()+2);
                                     break;
                                 default:
                             }
                         }
                     });
                 });
             }
             else if(self.ModeofShipment()==2 || self.ModeofShipment()==3)
             {
                 date2.setDate(date2.getDate()+3);
                 switch(weekDay) { // if 3 days
                     case 0: // Sunday
                     case 1: //Monday
                     case 6: //Saturday
                         //no day added
                         break;
                     case 2: //Tuesday
                     case 3: //Wednesday
                     case 4: //Thursday
                     case 5: //Friday
                         date2.setDate(date2.getDate()+1);
                         break;
                     default:
                 }
             }
             self.Job.DemurrageDate(date2);
             $('.d_demDate').datetimepicker('setDate', date2);
