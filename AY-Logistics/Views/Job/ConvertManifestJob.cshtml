                             date2.setDate(date2.getDate()+5);
                             switch(weekDay) { // if 5 days
                                 case 0: // Sunday
                                 case 1: //Monday
                                 case 2: //Tuesday
                                 case 3: //Wednesday
                                 case 4: //Thursday
                                 case 5: //Friday
                                     date2.setDate(date2.getDate()+1);
